import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
	    Scanner sc = new Scanner(System.in);
	    String s1 = sc.nextLine();
	    String s2 = sc.nextLine();
	    char[] c1 = s1.toCharArray();
	    char[] c2 = s2.toCharArray();
		int i,j,newarr;
		for(i=0;i<c1.length;i++){
		    for(j=0;j<c2.length;j++){
		       newarr = c1[i]+c2[j];
		       return newarr;
		    }
		}
		return 0;
	}
}
