class GfG
{
	public int remAnagrams(String s,String s1)
        {
        String rem = new String();
        int count = 0;
        char[] r = rem.toCharArray();
        char[] sc = s.toCharArray();
        char[] sc1 = s1.toCharArray();
        for(int i = 0; i<sc.length-1; i++){
            for(int j = 0; j<sc1.length-1; j++){
                if(sc[i]==sc1[j])
                   r[i] = sc[i];
                   count++;
            }
            
        }
        return count;
        }
}
