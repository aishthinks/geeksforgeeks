class GfG
{
    /*You are required to complete this method
    * ar1 : 1st array
    * ar2 : 2nd array
    */
    int maxPathSum(int ar1[], int ar2[])
    {
        int result = 0, sum1 = 0, sum2 = 0;
        for(int i = 0; i<m; i++){
            for(int j = 0; j<n; j++){
                if(ar1[i]<ar2[j])
                  sum1++
                else
                  sum2++
            }
        }
    }
}
