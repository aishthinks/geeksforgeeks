class Solution
{
    public int firstElementKTime(int[] a, int n, int k) { 
       int count = 0; 
       for(int i = 0; i < n ; i++){
           if(a[i]==a[i+1])
             count++;
           while(count == k){
           break;
       }
       return i;
       }
       
       return 0;
    } 
}
