class Solution
{
    static void transpose(int matrix[][], int n)
    {
        int i, j,temp;
        for(i = 0; i<n; i++){
            for(j = i; j<n; j++){
                temp=matrix[i][j];
                matrix[i][j] = matrix[j][i];
                matrix[j][i] = temp;
            }
        }
    }
}
